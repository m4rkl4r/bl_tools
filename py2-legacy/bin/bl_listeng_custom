#!/usr/bin/env bash
. /unixworks/bl_tools/etc/bl_tools.sh

if test -z "$BL_ENG_PROFILE"; then
    echo Please define BL_ENG_PROFILE in \~/.bl_tools/blvars.sh
    exit 1
fi
bl_conf "$BL_ENG_PROFILE" > /dev/null
bl_lsdir.u --servergroup="/Servers by Role/$BL_ENG_ROLE" | egrep "^\s" | sed -e 's/"\|\s//g' | egrep -v "^$" | grep -v IDgroupdescription
